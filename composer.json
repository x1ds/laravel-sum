{
    "name": "nazububu/laravel-sum",
    "description": "Laravel wrapper for simple PHP package for add two numbers",
    "license": "MIT",
    "keywords": [
        "laravel",
        "sum",
        "laravel-sum"
    ],
    "authors": [
        {
            "name": "Nazar Tertyshnyi",
            "email": "nazar.tertyshny@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/nazububu/laravel-sum/issues",
        "source": "https://github.com/nazububu/laravel-sum"
    },
    "require": {
        "php": "^7.1",
        "illuminate/support": "*",
        "nazububu/sum": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Nazububu\\LaravelSum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nazububu\\LaravelSum\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
